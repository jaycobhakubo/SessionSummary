                                     HorizontalContentAlignment="Right"
                                     Text="{Binding PrizeMerchandise, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="18"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Total other prizes:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
