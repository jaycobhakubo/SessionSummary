                                   VerticalAlignment="Bottom"
                                   Content="{Binding SaleAdjustTotal, UpdateSourceTrigger=PropertyChanged}" />
                            <Label Grid.Row="14"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Foreground="{DynamicResource BlueBrush}"
                                   Content=""
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <Label Grid.Row="15"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="Total cash prizes:"
                                   Padding="0"
                                   VerticalAlignment="Bottom" />
                            <TextBox Grid.Row="15"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding PrizeCash, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="16"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Total check prizes:"
