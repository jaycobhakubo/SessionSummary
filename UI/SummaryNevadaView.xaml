                                 SpellCheck.IsEnabled="True"
                                 TextWrapping="Wrap"
                                 AcceptsReturn="True"
                                 MaxLength="{x:Static base:StringSizes.MaxCommentLength}"
                                 IsEnabled="{Binding CanEditSummaryData}"
                                 Text="{Binding Comments, UpdateSourceTrigger=PropertyChanged}"
                                 HorizontalAlignment="Stretch"
                                 VerticalAlignment="Stretch" />
                    </Grid>
                </Grid>
                <Border Margin="20,2,10,0"
                        Style="{DynamicResource SemiTransparentNoBorder}"
                        Grid.Column="1">
                    <ScrollViewer VerticalScrollBarVisibility="Auto">
                        <Grid Margin="0,0,0,0"
                              IsEnabled="{Binding AllowModifications}">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.667*" />
                                <ColumnDefinition Width="0.35*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="Paper sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0"
                                   HorizontalAlignment="Right" />
                            <TextBox Grid.Row="0"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SalePaper, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="1"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content=" (+) Electronic sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="1"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleElec, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="2"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Bingo (other) sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="2"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleBingoOther, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="3"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Pull tab sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="3"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SalePT, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom"
                                     Padding="1" />
                            <Label Grid.Row="4"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Concession sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="4"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleConc, UpdateSourceTrigger=PropertyChanged}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="5"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Mechandise sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="5"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleMrch, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="6"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Validation Sales:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="6"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleValid, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="7"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(-) Discounts and coupons:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="7"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding SaleDisc, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="8"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Total tax collected:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="8"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding ExpectTax, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="9"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(+) Device fees charged:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <TextBox Grid.Row="9"
                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
