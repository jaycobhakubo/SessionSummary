                                     Grid.Column="1"
                                     Margin="1"
                                     ui:TextBoxMaskBehavior.Mask="Decimal"
                                     ui:TextBoxMaskBehavior.Precision="2"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     HorizontalContentAlignment="Right"
                                     Text="{Binding ExpectTax, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                     VerticalAlignment="Bottom" />
                            <Label Grid.Row="12"
                                   Margin="1,1,5,5"
                                   HorizontalContentAlignment="Right"
                                   Content="(-) Device fees charged:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
