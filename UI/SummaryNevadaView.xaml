<UserControl x:Class="GameTech.Elite.Client.Modules.SessionSummary.UI.SummaryNevadaView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:base="clr-namespace:GameTech.Elite.Base;assembly=GameTech.Elite.Base"
             xmlns:ui="clr-namespace:GameTech.Elite.UI;assembly=GameTech.Elite.UI"
             xmlns:lc="clr-namespace:GameTech.Elite.Client.Modules.SessionSummary.UI.Converters"
             mc:Ignorable="d" d:DesignHeight="819" d:DesignWidth="1003">
    <UserControl.Resources>
        <ui:BooleanInverterConverter x:Key="boolInverter" />
        <BooleanToVisibilityConverter x:Key="VisibilityConverter" />
        <lc:AllBooleansSetConverter x:Key="AllBoolToBoolConverter" />
    </UserControl.Resources>
    <Border Style="{DynamicResource LightBlueBorder}"
            Margin="5">
        <Grid Margin="0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto"
                               MinHeight="31" />
                <RowDefinition Height="Auto"
                               MinHeight="84" />
            </Grid.RowDefinitions>
            <Grid Margin="15,0,0,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Grid Margin="0,5,0,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.20*" />
                        <RowDefinition Height="0.300*" />
                        <RowDefinition Height="0.500*" />
                    </Grid.RowDefinitions>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Label Content="Gaming date:"
                               VerticalAlignment="Bottom"
                               Padding="0"
                               Margin="0,0,0,3" />
                        <DatePicker x:Name="m_gameDate"
                                    Grid.Row="1"
                                    IsEnabled="{Binding CanEditDate}"
                                    SelectedDate="{Binding GamingDate, UpdateSourceTrigger=LostFocus, ValidatesOnDataErrors=True}"
                                    Margin="0,0,10,0"
                                    Width="114"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left" />
                        <Label Grid.Column="1"
                               Content="Session:"
                               VerticalAlignment="Bottom"
                               Padding="0"
                               Margin="0,0,0,3" />
                        <ComboBox Grid.Row="1"
                                  Grid.Column="1"
                                  ItemsSource="{Binding Sessions, UpdateSourceTrigger=PropertyChanged}"
                                  SelectedItem="{Binding SelectedSession, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                  IsEnabled="{Binding CanEditSession, UpdateSourceTrigger=PropertyChanged}"
                                  VerticalAlignment="Center"
                                  Margin="0,0,60,0" />
                        <Label Visibility="Collapsed"
                               Grid.Row="2"
                               Content="System attendance:" />
                        <Label Visibility="Collapsed"
                               Grid.Row="2"
                               Grid.Column="1"
                               Content="Time calculated:" />
                        <Label Visibility="Collapsed"
                               Grid.Row="3"
                               Content="{Binding AttendSystem, UpdateSourceTrigger=PropertyChanged}" />
                        <Label Visibility="Collapsed"
                               Grid.Row="3"
                               Grid.Column="1"
                               Content="{Binding AttendSystemTime, UpdateSourceTrigger=PropertyChanged}" />
                        <Label Grid.Row="2"
                               Content="Manual attendance:"
                               Padding="0"
                               Margin="0,0,0,3"
                               VerticalAlignment="Bottom" />
                        <Label Grid.Row="2"
                               IsEnabled="{Binding AllowModifications}"
                               Grid.Column="1"
                               Content="Time calculated:"
                               VerticalAlignment="Bottom"
                               Padding="0"
                               Margin="0,0,0,3" />
                        <TextBox Grid.Row="5"
                                 ui:TextBoxMaskBehavior.Mask="Integer"
                                 ui:TextBoxMaskBehavior.MinimumValue="0"
                                 IsEnabled="{Binding CanEditSummaryData}"
                                 Text="{Binding AttendManual, UpdateSourceTrigger=PropertyChanged, ValidatesOnDataErrors=True}"
                                 Margin="0,0,40,0"
                                 VerticalAlignment="Center"
                                 Width="80"
                                 Padding="1"
                                 HorizontalAlignment="Left" />
                        <Grid Grid.Row="5"
                              Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.3*" />
                                <ColumnDefinition Width="0.1*" />
                                <ColumnDefinition Width="0.3*" />
                                <ColumnDefinition Width="0.05*" />
                                <ColumnDefinition Width="0.35*" />
                            </Grid.ColumnDefinitions>
                            <ComboBox Margin="0"
                                      IsEnabled="{Binding AllowModifications}"
                                      SelectedIndex="{Binding AttendManualTimeHrIndex, UpdateSourceTrigger=PropertyChanged}"
                                      VerticalAlignment="Center"
                                      ItemsSource="{Binding TimeHrs}" />
                            <Label HorizontalContentAlignment="Center"
                                   Grid.Column="1"
                                   Content="{Binding TimeSeparator}" />
                            <ComboBox Margin="0"
                                      Grid.Column="2"
                                      IsEnabled="{Binding AllowModifications}"
                                      SelectedIndex="{Binding AttendManualTimeMinIndex, UpdateSourceTrigger=PropertyChanged}"
                                      VerticalAlignment="Center"
                                      ItemsSource="{Binding TimeMins}" />
                            <ComboBox Margin="0"
                                      Grid.Column="4"
                                      IsEnabled="{Binding AllowModifications}"
                                      Visibility="{Binding Is12HrMode, Converter={StaticResource VisibilityConverter}}"
                                      SelectedIndex="{Binding AttendManualTimeAMPMIndex, UpdateSourceTrigger=PropertyChanged}"
                                      VerticalAlignment="Center"
                                      ItemsSource="{Binding TimeAMPMDesignators}" />
                        </Grid>
                    </Grid>
                    <Grid Grid.Row="1"
                          Margin="0,10,0,0">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <UniformGrid Rows="2"
                                     Columns="3">
                            <Label Content="Player spend:"
                                   VerticalAlignment="Bottom"
                                   Padding="0"
                                   Margin="0,0,0,3" />
                            <Label Content="Bingo spend:"
                                   VerticalAlignment="Bottom"
                                   Padding="0"
                                   Margin="0,0,0,3" />
                            <Label Content="Pull tab spend:"
                                   VerticalAlignment="Bottom"
                                   Padding="0"
                                   Margin="0,0,0,3" />
                            <Label Content="{Binding SpendTotal, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalContentAlignment="Left" />
                            <Label Content="{Binding SpendBingoTotal, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalContentAlignment="Left" />
                            <Label Content="{Binding SpendPTTotal, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalContentAlignment="Left" />
                        </UniformGrid>
                        <UniformGrid Grid.Row="1"
                                     Rows="2"
                                     Columns="2">
                            <Label Content="Merchandise spend:"
                                   VerticalAlignment="Bottom"
                                   Padding="0"
                                   Margin="0,0,0,3" />
                            <Label Content="Concession spend:"
                                   VerticalAlignment="Bottom"
                                   Margin="0,0,0,3"
                                   Padding="0" />
                            <Label Content="{Binding SpendMrchTotal, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left" />
                            <Label Content="{Binding SpendConcTotal, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left" />
                        </UniformGrid>
                        <Grid Grid.Row="3">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".4*" />
                                <ColumnDefinition Width=".25*" />
                                <ColumnDefinition Width=".1*" />
                                <ColumnDefinition Width=".25*" />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Row="0"
                                   Grid.Column="0"
                                   Content="Accrual Based Payout:"
                                   VerticalAlignment="Bottom"
                                   Margin="0,0,0,3"
                                   Padding="0" />
                            <Label Grid.Row="0"
                                   Grid.Column="1"
                                   Content="{Binding SpendPercentPayAccrualBased, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Bottom" />
                            <Label Grid.Row="0"
                                   Grid.Column="2"
                                   Content="Hold:"
                                   Margin="0,0,0,3"
                                   Padding="0"
                                   VerticalAlignment="Bottom" />
                            <Label Grid.Row="0"
                                   Grid.Column="3"
                                   Content="{Binding SpendPercentHoldAccrualBased, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Bottom" />
                            <Label Grid.Row="1"
                                   Grid.Column="0"
                                   Content="Cash Based Payout:"
                                   VerticalAlignment="Bottom"
                                   Margin="0,0,0,3"
                                   Padding="0" />
                            <Label Grid.Row="1"
                                   Grid.Column="1"
                                   Content="{Binding SpendPercentPayCashBased, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Bottom" />
                            <Label Grid.Row="1"
                                   Grid.Column="2"
                                   Content="Hold:"
                                   Margin="0,0,0,3"
                                   Padding="0"
                                   VerticalAlignment="Bottom" />
                            <Label Grid.Row="1"
                                   Grid.Column="3"
                                   Content="{Binding SpendPercentHoldCashBased, UpdateSourceTrigger=PropertyChanged}"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Bottom" />
                        </Grid>
                    </Grid>
                    <Grid Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.606*" />
                            <RowDefinition Height="0.1*" />
                            <RowDefinition Height="0.294*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.200*" />
                                <RowDefinition Height="0.235*" />
                                <RowDefinition Height="0.200*" />
                                <RowDefinition Height="0.255*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.787*" />
                                <ColumnDefinition Width="0.213*" />
                            </Grid.ColumnDefinitions>
                            <Label Margin="1,0,0,3"
                                   Content="Supervisor:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <ComboBox Margin="1"
                                      Grid.Row="1"
                                      IsEnabled="{Binding CanEditSummaryData}"
                                      SelectedItem="{Binding SelectedManager, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Managers, UpdateSourceTrigger=PropertyChanged}"
                                      VerticalAlignment="Top" />
                            <Label Margin="1,0,0,3"
                                   Grid.Row="2"
                                   Content="Caller:"
                                   VerticalAlignment="Bottom"
                                   Padding="0" />
                            <ComboBox Margin="1"
                                      Grid.Row="3"
                                      IsEnabled="{Binding CanEditSummaryData}"
                                      SelectedItem="{Binding SelectedCaller, UpdateSourceTrigger=PropertyChanged}"
                                      ItemsSource="{Binding Callers, UpdateSourceTrigger=PropertyChanged}"
                                      VerticalAlignment="Top" />
                            <Button Margin="5,1,1,1"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Content="Add"
                                    Command="{Binding AddCallerCommand}"
                                    Height="25"
                                    VerticalAlignment="Top" />
                            <ListBox Margin="0"
                                     Grid.Row="4"
                                     Height="45"
                                     IsEnabled="{Binding CanEditSummaryData}"
                                     SelectedItem="{Binding SelectedChoosenCaller, UpdateSourceTrigger=PropertyChanged}"
                                     ItemsSource="{Binding ChoosenCallers, UpdateSourceTrigger=PropertyChanged}"
                                     VerticalAlignment="Top" />
                            <Button Margin="5,1,1,1"
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    Content="Delete"
                                    Command="{Binding DeleteCallerCommand}"
                                    VerticalAlignment="Bottom"
                                    Height="25" />
                        </Grid>
                        <Label Grid.Row="1"
                               Content="Comments:"
                               VerticalAlignment="Bottom"
                               Padding="0"
                               Margin="0,0,0,3" />
